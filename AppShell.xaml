<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiChatAppdeux.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiChatAppdeux"
    Shell.FlyoutBehavior="Disabled">

    <TabBar Route="profile">


        <ShellContent Route="main" ContentTemplate="{DataTemplate local:HomeView}" />
        
        <ShellContent  Route="register" ContentTemplate="{DataTemplate local:RegisterPage}" />
       
    </TabBar>
    <TabBar Route="middle">
        <ShellContent  Route="chatarea" ContentTemplate="{DataTemplate local:ChatRooms}" />
    </TabBar>
    <TabBar Route="inside">
        <ShellContent  Route="chat" ContentTemplate="{DataTemplate local:ChatPage}" />
        <ShellContent  Route="chat1" ContentTemplate="{DataTemplate local:Chat1Page}" />
        <ShellContent  Route="chat2" ContentTemplate="{DataTemplate local:Chat2Page}" />
        <ShellContent  Route="chat3" ContentTemplate="{DataTemplate local:Chat3Page}" />
        <ShellContent  Route="chat4" ContentTemplate="{DataTemplate local:Chat4Page}" />
        <ShellContent  Route="Dash" ContentTemplate="{DataTemplate local:Dashboard}" />
    </TabBar>
</Shell>
