<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiChatAppdeux.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiChatAppdeux"
    Shell.FlyoutBehavior="Disabled">

    <TabBar Route="profile">


        <ShellContent Route="main" ContentTemplate="{DataTemplate local:HomeView}" />
        <ShellContent  Route="chatarea" ContentTemplate="{DataTemplate local:ChatRooms}" />
        <ShellContent  Route="register" ContentTemplate="{DataTemplate local:RegisterPage}" />
        <ShellContent  Route="chat" ContentTemplate="{DataTemplate local:ChatPage}" />
    </TabBar>
</Shell>
